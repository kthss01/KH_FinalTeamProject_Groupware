					console.log(list);
					$.each(list,function(i,obj){

						categoryBox.append(`
								<li class="sidebar-item">
                                <a href="boardList.co?cno=\${ obj.cno }&cname=\${ obj.cname }" class="sidebar-link ">
                                	<span class="hide-menu"> \${ obj.cname } </span>
                                </a>
                                </li>
								`) 
			 		}) 				
				}
			})
			
		})
